# aws parameters
export STEWARD_REGION=eu-west-3
export STEWARD_AZ=eu-west-3a
export STEWARD_CLUSTER_VERSION="1.32"
export STEWARD_CLUSTER=steward-2
export STEWARD_NODE_NG=${STEWARD_CLUSTER}-${STEWARD_REGION}-ng1
export STEWARD_NODE_TYPE=t3.medium
export STEWARD_VPC_NAME=eksctl-${STEWARD_CLUSTER}-cluster/VPC
export STEWARD_VPC_CIDR=192.168.0.0/16
export STEWARD_PUBLIC_SUBNET_NAME=eksctl-${STEWARD_CLUSTER}-cluster/SubnetPublicEUWEST3A
export STEWARD_PRIVATE_SUBNET_NAME=eksctl-${STEWARD_CLUSTER}-cluster/SubnetPrivateEUWEST3A


# steward parameters
export STEWARD_NAMESPACE=steward-system

# tenant cluster parameters
export TENANT_NAMESPACE=tenant-00
export TENANT_NAME=tenant-00
export TENANT_DOMAIN=internal.steward.example.com
export TENANT_VERSION=v1.31.0
export TENANT_PORT=6443 # port used to expose the tenant api server
export TENANT_PROXY_PORT=8132 # port used to expose the konnectivity server
export TENANT_POD_CIDR=10.36.0.0/16
export TENANT_SVC_CIDR=10.96.0.0/16
export TENANT_DNS_SERVICE=10.96.0.10

export TENANT_VM_SIZE=t3.medium
export TENANT_ASG_MIN_SIZE=1
export TENANT_ASG_MAX_SIZE=1
export TENANT_ASG_DESIRED_SIZE=1
export TENANT_SUBNET_ADDRESS=10.0.4.0/24
export TENANT_ASG_NAME=$TENANT_NAME-workers
